{
  "name": "hirez-signature-helper",
  "version": "0.0.1",
  "description": "Package to help create a signature",
  "exports": {
    "import": "./lib/esm/hirez-signature-helper.js",
    "require": "./lib/cjs/hirez-signature-helper.js",
    "default": "./lib/esm/hirez-signature-helper.js"
  },
  "types": "./lib/esm/hirez-signature-helper.d.ts",
  "module": "true",
  "scripts": {
    "test": "TZ=UTC vitest",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "prebuild": "rimraf lib"
  },
  "keywords": [
    "hirez",
    "smite",
    "paladins"
  ],
  "author": "Wander Souza",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "vitest": "^0.25.2"
  }
}
